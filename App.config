<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--  RabbitMQ ServiceBus Configurations  -->
    <add key="SBmemoryExchange" value="Avitia.Exchange" />    <!--  inMemory Client Exchange Name - no connection required   -->
    
    <add key="SBlocalExchange" value="Local.Avitia.Exchange" />    <!--  local persistent Exchange Name   -->
    <add key="SBLocalConnection" value="amqp://guest:guest@localhost:5672/" />

  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

</configuration>